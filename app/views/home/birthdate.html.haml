= render :partial => 'header'
.row
  .eight.columns      
    = form_tag contact_info_path(@app) do
      %fieldset
        %legend
          When were you born?
        %ol
          %li
            = select_date Date.parse(session[:user][:date_of_birth]), :start_year => 1900, :end_year => Date.current.year, :date_separator => "</li><li>".html_safe, :default => { :year => 1998, :month => 1, :day => 1 }
      %div.actions
        = submit_tag 'Continue', :class => 'large button'